</span><span><span class="hljs-keyword">from</span></span><span> pathlib </span><span><span class="hljs-keyword">import</span></span><span> Path

meta_path = Path(</span><span><span class="hljs-string">"semantic-infrastructure/simulations/metadata.yaml"</span></span><span>)
output_tex = Path(</span><span><span class="hljs-string">"semantic-infrastructure/chapters/simulation_index.tex"</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">generate_index</span></span><span>():
    data = yaml.safe_load(meta_path.read_text())
    blocks = []
    </span><span><span class="hljs-keyword">for</span></span><span> key, entry </span><span><span class="hljs-keyword">in</span></span><span> data.items():
        blocks.append(<span class="hljs-string">f"""
