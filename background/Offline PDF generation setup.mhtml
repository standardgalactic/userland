TOPICS = [
    </span><span><span class="hljs-string">"Entropy and the Semantic Merge Operator"</span></span><span>,
    </span><span><span class="hljs-string">"Sheaf-Theoretic Intelligence"</span></span><span>,
    </span><span><span class="hljs-string">"Haskell as a Category of Meaning"</span></span><span>,
