ENV DEBIAN_FRONTEND=noninteractive

# Install dependencies
RUN apt-get update &amp;&amp; apt-get install -y \
    texlive-full \
    texlive-fonts-extra \
    python3 python3-pip \
    git wget curl \
    ghc cabal-install \
    build-essential \
