</span><span><span class="hljs-keyword">from</span></span><span> pathlib </span><span><span class="hljs-keyword">import</span></span><span> Path

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">check_chapters_and_appendices</span></span><span>(</span><span><span class="hljs-params">base_dir=<span class="hljs-string">"semantic-infrastructure"</span></span></span><span>):
    </span><span><span class="hljs-keyword">for</span></span><span> folder </span><span><span class="hljs-keyword">in</span></span><span> [</span><span><span class="hljs-string">"chapters"</span></span><span>, </span><span><span class="hljs-string">"appendices"</span></span><span>]:
        </span><span><span class="hljs-keyword">for</span></span><span> file </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">sorted</span></span><span>(Path(</span><span><span class="hljs-string">f"<span class="hljs-subst">{base_dir}</span></span></span><span>/</span><span><span class="hljs-subst">{folder}</span></span><span>").glob(</span><span><span class="hljs-string">"*.tex"</span></span><span>)):
            </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> file.read_text().strip():
                </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"❌ Empty file: <span class="hljs-subst">{file}</span></span></span><span>")

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">check_bib_entries</span></span><span>(</span><span><span class="hljs-params">bib_path=<span class="hljs-string">"semantic-infrastructure/bibliography.bib"</span></span></span><span>):
    text = Path(bib_path).read_text()
    entries = text.count(</span><span><span class="hljs-string">"@"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> entries &lt; </span><span><span class="hljs-number">5</span></span><span>:
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"⚠️ Low bibliography entry count (<span class="hljs-subst">{entries}</span></span></span><span>). Add more citations?")

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">check_simulation_output</span></span><span>():
    output_path = Path(</span><span><span class="hljs-string">"semantic-infrastructure/simulations/output/phi_field.png"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-keyword">not</span></span><span> output_path.exists():
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"❌ Simulation output missing: phi_field.png"</span></span><span>)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"✅ Simulation plot exists."</span></span><span>)

</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">main</span></span><span>():
