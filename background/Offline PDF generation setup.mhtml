</span><span><span class="hljs-comment"># Create a 5D lattice</span></span><span>
L = </span><span><span class="hljs-number">4</span></span><span>
shape = (L, L, L, L, L)
spins = np.random.choice([-</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>], size=shape)

</span><span><span class="hljs-comment"># Simple local update rule (mean-field for now)</span></span><span>
</span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">update</span></span><span>(</span><span><span class="hljs-params">spins</span></span><span>):
    </span><span><span class="hljs-keyword">for</span></span><span> idx </span><span><span class="hljs-keyword">in</span></span><span> np.ndindex(*spins.shape):
        neighbors = []
        </span><span><span class="hljs-keyword">for</span></span><span> d </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(</span><span><span class="hljs-number">5</span></span><span>):
            </span><span><span class="hljs-keyword">for</span></span><span> offset </span><span><span class="hljs-keyword">in</span></span><span> [-</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">1</span></span><span>]:
                i = </span><span><span class="hljs-built_in">list</span></span><span>(idx)
                i[d] = (i[d] + offset) % L
                neighbors.append(spins[</span><span><span class="hljs-built_in">tuple</span></span><span>(i)])
        local_field = </span><span><span class="hljs-built_in">sum</span></span><span>(neighbors)
        spins[idx] = </span><span><span class="hljs-number">1</span></span><span> </span><span><span class="hljs-keyword">if</span></span><span> np.random.rand() &lt; </span><span><span class="hljs-number">1</span></span><span> / (</span><span><span class="hljs-number">1</span></span><span> + np.exp(-</span><span><span class="hljs-number">2</span></span><span> * local_field)) </span><span><span class="hljs-keyword">else</span></span><span> -</span><span><span class="hljs-number">1</span></span><span>

</span><span><span class="hljs-comment"># Run simulation</span></span><span>
</span><span><span class="hljs-keyword">for</span></span><span> t </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(</span><span><span class="hljs-number">100</span></span><span>):
    update(spins)

</span><span><span class="hljs-comment"># Save average magnetization</span></span><span>
np.save(</span><span><span class="hljs-string">"simulations/output/ising5d.npy"</span></span><span>, spins.mean())
</span></span></code></div></div></pre>
