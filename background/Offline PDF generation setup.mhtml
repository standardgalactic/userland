</span><span><span class="hljs-keyword">\[</span></span><span>
</span><span><span class="hljs-keyword">\mathcal</span></span><span>{M} </span><span><span class="hljs-keyword">\to</span></span><span> </span><span><span class="hljs-keyword">\mathcal</span></span><span>{O}</span><span><span class="hljs-built_in">_</span></span><span>1, </span><span><span class="hljs-keyword">\quad</span></span><span> </span><span><span class="hljs-keyword">\mathcal</span></span><span>{M} </span><span><span class="hljs-keyword">\to</span></span><span> </span><span><span class="hljs-keyword">\mathcal</span></span><span>{O}</span><span><span class="hljs-built_in">_</span></span><span>2, </span><span><span class="hljs-keyword">\quad</span></span><span> </span><span><span class="hljs-keyword">\dots</span></span><span>
</span><span><span class="hljs-keyword">\]</span></span><span>

