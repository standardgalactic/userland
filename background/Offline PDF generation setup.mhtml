name: Build and Deploy GitHub Pages

on:
  push:
    branches: [ main ]

</span><span><span class="hljs-built_in">jobs</span></span><span>:
  deploy:
    runs-on: ubuntu-latest

    steps:
      - uses: actions/checkout@v3

      - name: Install LaTeX and Python
        run: |
          sudo apt-get update
          sudo apt-get install -y texlive-full python3 python3-pip dvisvgm latexmk
          pip3 install numpy matplotlib scipy

      - name: Build PDF
        run: |
          </span><span><span class="hljs-built_in">cd</span></span><span> semantic-infrastructure
          latexmk -pdf main.tex
          </span><span><span class="hljs-built_in">cp</span></span><span> main.pdf ../docs/downloads/Semantic_Infrastructure_Monograph.pdf

      - name: Export Diagrams to SVG
        run: |
          </span><span><span class="hljs-built_in">cd</span></span><span> semantic-infrastructure/diagrams
          </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> *.tex; </span><span><span class="hljs-keyword">do</span></span><span> \
            latex </span><span><span class="hljs-string">"<span class="hljs-variable">$f</span></span></span><span>" &amp;&amp; dvisvgm </span><span><span class="hljs-string">"<span class="hljs-variable">${f%.tex}</span></span></span><span>.dvi" -o </span><span><span class="hljs-string">"../../docs/diagrams/<span class="hljs-variable">${f%.tex}</span></span></span><span>.svg"; \
          </span><span><span class="hljs-keyword">done</span></span><span>

      - name: Deploy to GitHub Pages
        uses: peaceiris/actions-gh-pages@v3
        with:
          github_token: </span><span><span class="hljs-variable">${{ secrets.GITHUB_TOKEN }</span></span><span>}
          publish_dir: ./docs
EOF

</span><span><span class="hljs-comment"># Create Makefile</span></span><span>
</span><span><span class="hljs-built_in">cat</span></span><span> &gt; Makefile &lt;&lt;</span><span><span class="hljs-string">'EOF'</span></span><span>
PDF=build/Semantic_Infrastructure_Monograph.pdf
MAIN=main
LATEX=pdflatex
BIB=bibtex

.PHONY: all clean diagrams svg-diagrams haskell sim pages

all: $(PDF)

$(PDF): main.tex chapters/*.tex appendices/*.tex diagrams/*.pdf bibliography.bib
	$(LATEX) $(MAIN)
	$(BIB) $(MAIN)
	$(LATEX) $(MAIN)
	$(LATEX) $(MAIN)
	</span><span><span class="hljs-built_in">mkdir</span></span><span> -p build
	</span><span><span class="hljs-built_in">mv</span></span><span> $(MAIN).pdf $(PDF)

diagrams:
	</span><span><span class="hljs-built_in">cd</span></span><span> diagrams &amp;&amp; </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> *.tex; </span><span><span class="hljs-keyword">do</span></span><span> pdflatex $</span><span><span class="hljs-variable">$f</span></span><span>; </span><span><span class="hljs-keyword">done</span></span><span>

svg-diagrams:
	</span><span><span class="hljs-built_in">cd</span></span><span> diagrams &amp;&amp; </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> *.tex; </span><span><span class="hljs-keyword">do</span></span><span> \
	  latex </span><span><span class="hljs-string">"$<span class="hljs-variable">$f</span></span></span><span>" &amp;&amp; dvisvgm </span><span><span class="hljs-string">"$<span class="hljs-variable">${f%.tex}</span></span></span><span>.dvi" -o </span><span><span class="hljs-string">"../docs/diagrams/$<span class="hljs-variable">${f%.tex}</span></span></span><span>.svg"; \
	</span><span><span class="hljs-keyword">done</span></span><span>

haskell:
	</span><span><span class="hljs-built_in">cd</span></span><span> haskell &amp;&amp; ./run_haskell.sh

sim:
	</span><span><span class="hljs-built_in">cd</span></span><span> simulations &amp;&amp; python3 rsvp_simulation.py

pages:
	</span><span><span class="hljs-built_in">mkdir</span></span><span> -p docs/downloads
	</span><span><span class="hljs-built_in">cp</span></span><span> build/Semantic_Infrastructure_Monograph.pdf docs/downloads/
EOF

</span><span><span class="hljs-comment"># Create Python validator</span></span><span>
</span><span><span class="hljs-built_in">cat</span></span><span> &gt; scripts/check_project.py &lt;&lt;</span><span><span class="hljs-string">'EOF'</span></span><span>
import os
from pathlib import Path

def check_chapters_and_appendices(base_dir=</span><span><span class="hljs-string">"semantic-infrastructure"</span></span><span>):
    </span><span><span class="hljs-keyword">for</span></span><span> folder </span><span><span class="hljs-keyword">in</span></span><span> [</span><span><span class="hljs-string">"chapters"</span></span><span>, </span><span><span class="hljs-string">"appendices"</span></span><span>]:
        </span><span><span class="hljs-keyword">for</span></span><span> file </span><span><span class="hljs-keyword">in</span></span><span> sorted(Path(f</span><span><span class="hljs-string">"{base_dir}/{folder}"</span></span><span>).glob(</span><span><span class="hljs-string">"*.tex"</span></span><span>)):
            </span><span><span class="hljs-keyword">if</span></span><span> not file.read_text().strip():
                </span><span><span class="hljs-built_in">print</span></span><span>(f</span><span><span class="hljs-string">"❌ Empty file: {file}"</span></span><span>)

def check_bib_entries(bib_path=</span><span><span class="hljs-string">"semantic-infrastructure/bibliography.bib"</span></span><span>):
    text = Path(bib_path).read_text()
    entries = text.count(</span><span><span class="hljs-string">"@"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> entries &lt; 5:
        </span><span><span class="hljs-built_in">print</span></span><span>(f</span><span><span class="hljs-string">"⚠️ Low bibliography entry count ({entries}). Add more citations?"</span></span><span>)

def check_simulation_output():
    output_path = Path(</span><span><span class="hljs-string">"semantic-infrastructure/simulations/output/phi_field.png"</span></span><span>)
    </span><span><span class="hljs-keyword">if</span></span><span> not output_path.exists():
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"❌ Simulation output missing: phi_field.png"</span></span><span>)
    </span><span><span class="hljs-keyword">else</span></span><span>:
        </span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">"✅ Simulation plot exists."</span></span><span>)

def main():
    check_chapters_and_appendices()
    check_bib_entries()
    check_simulation_output()

</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    main()
EOF

