semanticMerge (m1, m2) =
  let
    delta = m1 `restrictedTo` overlap `diff` m2 `restrictedTo` overlap
    obstruction = computeExt1(cotangentComplex(m1,m2), tangentComplex(m1,m2))
  in
    if isZero obstruction
      then Right (glueSections m1 m2)
